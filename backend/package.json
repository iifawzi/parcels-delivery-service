{
  "dependencies": {
    "@types/express": "^4.17.15",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.8.1",
    "nodemon": "^2.0.20",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  },
  "name": "parcels-backend",
  "version": "1.0.0",
  "main": "index.ts",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.12",
    "jest": "^29.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3"
  },
  "scripts": {
    "test:e2e": "NODE_ENV=test jest --runInBand",
    "test:live": "NODE_ENV=integration-coverage jest --runInBand --detectOpenHandles",
    "test:e2e-cov": "NODE_ENV=test jest --coverageDirectory='e2e-coverage' --coverage --runInBand",
    "test:live-cov": "NODE_ENV=integration-coverage jest --coverage --coverageDirectory='integration-coverage' --runInBand",
    "test:merged-cov": "npx istanbul-merge --out ./coverage.json ./integration-coverage/coverage-final.json ./e2e-coverage/coverage-final.json && npx istanbul report --include coverage.json --dir full-coverage html",
    "test:clean-files": "rm -r ./e2e-coverage && rm -r ./integration-coverage && rm ./coverage.json",
    "test:cov": "npm run test:e2e-cov && npm run test:live-cov && npm run test:merged-cov && npm run test:clean-files",
    "start": "NODE_ENV=production  nodemon index.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "_moduleAliases": {
    "@": "."
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "./**/{!(ConsoleLogger.ts),}.ts"
    ],
    "testEnvironment": "node",
    "testTimeout": 10000,
    "modulePathIgnorePatterns": [
      "<rootDir>/di/"
    ]
  }
}
